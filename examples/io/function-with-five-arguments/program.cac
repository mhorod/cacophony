foreign write_int: [Int] -> Unit;

# idk if this should be a test, but it came out while debugging the segtree
let rec = [l: Int, r: Int, node: Int, nl: Int, nr: Int] -> Int => (
    if nr <= r then return 1;

    let mid = -1;
    return rec[l, r, node, nl, nr] + rec[l, r, node, mid, nr];
);
write_int[rec[0, 0, 1, 0, 0]];