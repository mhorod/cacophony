# integer operators
4 + 2;
4 - 2;
4 * 2;
4 / 2;
4 < 2;
4 > 2;
4 == 2;

# boolean operators
false && true;
true || false;

# nesting operators
((4 * 2) == (5 + 3)) && ((4 / 2) < (5 - 3)) || (3 < 8);

# initializing variable with arithmetic expression value
let x: Int = (4 + 5) + 7;
let y: Bool = (4 < 5) || (4 > 5);

# using arithmetic operators with variables
let a: Int = 2;
let b: Int = 1;
let c: Int = 3;
let d: Int = (a * c) + b;

